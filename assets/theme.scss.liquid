  bottom: 0;
  height: $slideshow-control-size;
  width: $slideshow-control-size;
  font-size: 0.6rem;
  line-height: 0.6rem;

  &:hover {
    bottom: 0.125rem;
  }

  & .icon {
    height: 0.4rem;
  }
}

.icon-pause {
  display: block;

  .is-paused & {
    display: none;
  }
