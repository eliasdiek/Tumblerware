.transit-container {
  position: absolute;
  bottom: 220px;
  width: 100%;
  background: #111;
  z-index: 100;
  height: 70px;
  display: flex;
  align-items: center;
  padding-top: 5px;
}
.transit-wrapper {
  width: fit-content;
  display: flex;
  align-items: flex-start;
  justify-content: space-between;

  .switch {
    position: relative;
    display: inline-block;
    width: 40px;
    height: 22px;

    @media screen and (max-width: 768px) {
      width: 32px;
    }

    input { 
      opacity: 0;
      width: 0;
      height: 0;

      &:checked + .slider {
        background-color: #9efce1;
      }
      &:focus + .slider {
        box-shadow: 0 0 1px #9efce1;
      }
      &:checked + .slider:before {
        transform: translateX(26px);

        @media screen and (max-width: 768px) {
          transform: translateX(18px);
        }
      }
    }

    .slider {
      position: absolute;
      cursor: pointer;
      top: 0;
      left: 0;
      right: 0;
      bottom: 0;
      background-color: #ccc;
      -webkit-transition: .4s;
      transition: .4s;

      &:before {
        position: absolute;
        content: "";
        height: 22px;
        width: 22px;
        left: -3px;
        bottom: 0;
        background-color: white;
        -webkit-transition: .4s;
        transition: .4s;
      }
      &.round {
        border-radius: 34px;

        &:before {
          border-radius: 50%;
        }
      }
    }
  }

  .checkbox-wrapper {
    margin-top: 3px;

    @media screen and (max-width: 768px) {
      margin-top: 0;
    }
  }

  .transit-content {
    margin-left: 10px;

    .transit-main-content {
      display: flex;
      align-items: center;
      justify-content: center;
      font-size: 16px;

      @media screen and (max-width: 768px) {
        align-items: center;
        font-size: 12px;
      }

      .transit-logo {
        color: #000;
        padding: 2px 5px;
        border-radius: 15px;
        font-size: 12px;
        display: inline-flex;
        align-items: center;
        justify-content: center;

        img {
          width: 60px;

          @media screen and (max-width: 768px) {
            width: 45px;
          }
        }
      }
    }
    .transit-referrence {
      margin-top: 5px;
      font-size: 13px;

      @media screen and (max-width: 768px) {
        font-size: 12px;
      }
      a {
        text-decoration: underline;
        &:hover {
          color: #fff;
        }
      }
    }
  }
}

.transit-modal-container {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 10000;
  height: 100vh;
  background: rgba(0, 0, 0, .5);
  display: none;
  overflow: auto;

  @media screen and (max-width: 768px) {
    padding: 25px;
  }

  .transit-modal-wrapper {
    max-width: 780px;
    margin: 100px auto;
    position: relative;

    @media screen and (max-width: 768px) {
      margin: 0 auto;
      margin-bottom: 100px;
    }

    #transit-close-btn {
      position: absolute;
      top: -5px;
      right: -5px;
      display: flex;
      background: #ccc;
      width: 25px;
      height: 25px;
      align-items: center;
      justify-content: center;
      border-radius: 12px;

      &:hover {
        opacity: .8;
      }
    }  

    .transit-modal-header {
      background-color: #9dfce1;
      padding: 45px 30px;
      text-align: center;
      .transit-title {
        font-family: "Futura condensed x-bold",sans-serif;
        color: #000;
        font-size: 48px;
        margin-bottom: 5px;
        line-height: 35px;
        letter-spacing: unset !important;
      }
      p {
        font-size: 14px;
        font-weight: 600;
        letter-spacing: unset !important;
        margin-bottom: 0;
      }
    }
    .transit-modal-content {
      background-color: #fff;
      padding: 45px 20px 10px;
      text-align: center;
      .modal-heading {
        color: #000;
        font-size: 34px;
        font-weight: 800;
        margin-bottom: 20px;
        letter-spacing: unset !important;
      }
      .transit-options-wrapper {
        display: flex;
        align-items: center;
        justify-content: center;
        flex-direction: row;

        @media screen and (max-width: 768px) {
          flex-direction: column;
        }

        .transit-option {
          padding: 20px;
          margin: 15px;
          border: 1px solid #a4a4a4;
          border-radius: 20px;
          min-width: 225px;

          @media screen and (max-width: 768px) {
            max-width: 100%;
            min-width: 100%;
          }

          .number {
            text-align: left;
            span {
              font-size: 42px;
              color: #a4a4a4;
              font-family: "Futura condensed x-bold",sans-serif;
              letter-spacing: unset !important;
            }
          }
          .transit-option-content {
            text-align: left;
            .option-heading {
              color: #000;
              font-size: 22px;
              font-weight: 800;
              line-height: 27px;
              letter-spacing: unset !important;
            }
            p {
              font-size: 12px;
              letter-spacing: unset !important;
            }
          }
        }
      }
      .transit-action-wrapper {
        margin: 50px auto 65px;
        text-align: center;

        .btn-buy_transit {
          border-radius: 30px !important;
          background-color: #9dfce1;
          font-size: 22px !important;
          color: #000;
          font-weight: 800 !important;
          padding: 10px 30px;
          letter-spacing: unset !important;

          &:disabled  {
            opacity: .5;
            cursor: not-allowed;
          }
        }
      }
    }
  }
}

