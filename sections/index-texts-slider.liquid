          {
            "type": "text",
            "id": "title",
            "label": "Add Texts Slider",
			"default": "Heading"
          }
        ]
      }
    ],
	"presets": [
      {
        "name": "Lander Slider Blocks",
        "category": "Text",
		"blocks": [
          {
            "type": "lander"
          },
		  {
            "type": "lander"
          }
        ]
      }
    ]
  }
{% endschema %}